<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="entidades.Falha" table="falha">
	
	  <id name="id" type="long" column="id" >
	   <generator class="increment"/>
	  </id>
	  
	  <property name="id_cliente" type="long" column="id_cliente"/>
	  
	  <property name="id_reclamacao" type="long" column="id_reclamacao"/>
	  
	  <property name="id_servico" type="long" column="id_servico"/>
	  
	  <property name="ativo" type="string" column="ativo" length="1"/>

   </class>

</hibernate-mapping>