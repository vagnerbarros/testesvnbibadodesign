<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="entidades.Reclamacao" table="reclamacao">
	
	  <id name="id" type="long" column="id" >
	   <generator class="increment"/>
	  </id>
	  
	  <property name="nome" type="string" column="nome" length="100"/>

	  <property name="codigo" type="string" column="codigo" length="5"/>
	  
	  <property name="id_empresa" type="long" column="id_empresa"/>
	  
	  <property name="ativo" type="string" column="ativo" length="1"/>
	  
   </class>

</hibernate-mapping>